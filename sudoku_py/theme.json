{
    "@lvl_label": {
        "colours": {
            "dark_bg": "#FFFFFF",
            "normal_text": "#000000",
            "disabled_bg": "#FFFFFF",
            "text_shadow": "#8F8F8F"
        }
    },
    "#title_label": {
        "font": {
            "name": "Nugo Sans",
            "size": "60",
            "regular_path": "font/NugoSansLight-9YzoK.ttf"
        }
    },
    "#subtitle_label": {
        "font": {
            "name": "Nugo Sans",
            "size": "20",
            "regular_path": "font/NugoSansLight-9YzoK.ttf"
        }
    },
    "@play_tile_btn": {
        "colours": {
            "normal_text": "#000000",
            "hovered_text": "#000000"
        },
        "font": {
            "name": "Montserrat",
            "size": "30",
            "regular_path": "font/Montserrat-Regular.ttf",
            "bold_path": "font/Montserrat-Bold.ttf",
            "bold": "1"
        },
        "misc": {
            "shape": "rectangle",
            "border_width": "0",
            "shadow_width": "0"
        }
    },
    "#shown_button": {
        "colours": {
            "normal_bg": "#CBCEDC",
            "hovered_bg": "#C0C3D1",
            "disabled_bg": "#CBCEDC",
            "selected_bg": "#CBCEDC",
            "active_bg": "#C0C3D1",
            "selected_text": "#000000",
            "active_text": "#000000"
        }
    },
    "#hidden_button": {
        "colours": {
            "normal_bg": "#FFFFFF",
            "hovered_bg": "#E0E5EC",
            "disabled_bg": "#FFFFFF",
            "selected_bg": "#BDDBFD",
            "active_bg": "#CECECE",
            "normal_text": "#315AAF",
            "hovered_text": "#315AAF",
            "selected_text": "#315AAF",
            "active_text": "#315AAF"
        }
    },
    "#wrong_button": {
        "colours": {
            "normal_bg": "#FFFFFF",
            "hovered_bg": "#E0E5EC",
            "disabled_bg": "#FFFFFF",
            "selected_bg": "#FDBDBD",
            "active_bg": "#CECECE",
            "normal_text": "#AF3131",
            "hovered_text": "#AF3131",
            "selected_text": "#AF3131",
            "active_text": "#AF3131"
        }
    },
    "@play_side_btn": {
        "font": {
            "name": "Montserrat",
            "size": "30",
            "regular_path": "font/Montserrat-Regular.ttf",
            "bold_path": "font/Montserrat-Bold.ttf",
            "bold": "1"
        }
    },
    "button": {
        "colours": {
            "normal_bg": "#FFFFFF",
            "hovered_bg": "#E0E5EC",
            "active_bg": "#CECECE",
            "normal_text": "#000000",
            "hovered_text": "#000000",
            "active_text": "#000000",
            "normal_border": "#000000",
            "hovered_border": "#000000",
            "active_border": "#000000"
        },
        "misc": {
            "shape": "rectangle",
            "border_width": "1",
            "shadow_width": "0"
        }
    },
    "@play_pbar": {
        "colours": {
            "normal_text": "#FFFFFF",
            "text_shadow": "#000000",
            "normal_border": "#000000",
            "filled_bar": "#8ac1ff,#158cd1,270",
            "unfilled_bar": "#CCCCCC"
        },
        "font": {
            "name": "Montserrat",
            "size": "14",
            "bold": "0",
            "italic": "1",
            "regular_path": "font/Montserrat-Regular.ttf",
            "bold_path": "font/Montserrat-Bold.ttf"
        },
        "misc": {
            "shape": "rounded_rectangle",
            "shape_corner_radius": "25",
            "shadow_width": "0"
        }
    },
    "defaults": {}
}